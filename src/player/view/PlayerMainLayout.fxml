<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="268.0" prefWidth="617.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.view.PlayerMainController">
   <children>
      <VBox layoutX="8.0" layoutY="9.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleStreamFrom" prefWidth="90.0" text="Stream from">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="saveToButton" mnemonicParsing="false" onAction="#handleSaveTo" prefWidth="90.0" text="Save to">
               <VBox.margin>
                  <Insets bottom="40.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handlePlay" prefWidth="90.0" text="Play">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleStop" prefWidth="90.0" text="Stop">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <CheckBox mnemonicParsing="false" text="Mute" />
         </children>
      </VBox>
      <HBox layoutX="108.0" layoutY="20.0" prefHeight="0.0" AnchorPane.leftAnchor="108.0">
         <children>
            <ComboBox fx:id="stationSelect" prefWidth="150.0">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ComboBox>
            <TextField fx:id="streamUrlField" prefHeight="25.0" prefWidth="299.0">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox layoutX="130.0" layoutY="64.0" prefHeight="25.0">
         <children>
            <TextField fx:id="streamToFilenameField" prefWidth="465.0" />
         </children>
      </HBox>
      <Label layoutX="130.0" layoutY="97.0" text="Message from server" />
      <TextArea layoutX="130.0" layoutY="114.0" prefHeight="122.0" prefWidth="452.0" />
   </children>
</AnchorPane>
